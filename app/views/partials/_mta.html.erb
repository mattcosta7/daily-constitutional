<%if @geo %>
  <%if @tStatus %>
    <div>
      <ul class='mta'>
        <% @tStatus.each do |x|%>
          <li>
            <%=image_tag x[0] %>- 
            <span>
              <%= x[1] == "GOOD SERVICE" ? x[1] : link_to("#{x[1]}", "#", class: 'service-status')%>
            </span>
            <span class='train-delay-status'  style="display:none;">
              <%=x[2].html_safe%>
            </span>
          </li>
        <%end%>
      </ul>
    </div>
  <%else%>
    <span>Shucks, we're not in your area yet, but we're working on it.</span>
  <%end%>
<%else%>
  <span>Location Could Not Be Determined.</span>
<%end%>