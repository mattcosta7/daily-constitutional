<div>
  <ul>
    <% @tStatus.each do |x|%>
      <li>
        <%=image_tag "#{x[0]}.gif" %>- 
        <span>
          <%= x[1] == "GOOD SERVICE" ? x[1] : link_to("#{x[1]}", "#")%>
        </span>
        <span class='train-delay-status'>
           <%=x[2].text.html_safe%>
        </span>
      </li>
    <%end%>
  </ul>
</div>