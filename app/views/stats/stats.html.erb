<%=collection_select :category, :category_id, Category.order(:title), :id, :title, {prompt: 'Select A Category'},id: 'map-select' %> -->
<ul>
<% @categories.each do |category| %>
  <%readers=[]%>
  <%category.blogs.each do |blog|%>
    <%blog.users.each do |user|%>
     <%readers<<user.email%>
    <%end%>
  <%end%>
<li>Category: <%=category.title%>:<%=readers.uniq.length%></li>
<%end%>
</ul>